const traducciones = require('./awajun.json');

function traducir(palabra) {
  return traducciones[palabra] || palabra;
}

console.log(traducir('atash'));   // gallo
console.log(traducir('nuwa'));    // mujer
console.log(traducir('aishmag')); // hombre
console.log(traducir('perro'));   // perro (no est√° en el diccionario, devuelve la palabra original)